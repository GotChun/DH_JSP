<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>

	<h1>쿠키확인하기</h1>
	<%
		Cookie[] cookies = request.getCookies(); //쿠키 배열로 만듬
		if(cookies!=null){
			for(Cookie cookie : cookies){
				%>
					<div>
					<%=cookie.getName() %> 	:	<%=cookie.getValue() %> 
					|
					<a href="">삭제하기</a>
					</div>
			<%
			}
		}
	%>


</body>
</html>